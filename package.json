  "scripts": {
    "build-static-stories": "build-storybook -c .storybook -o .storybook/static",
    "build-server": "babel server --out-dir .server --extends='server'",
    "build-ts": "tsc",
    "watch-ts": "yarn build-ts --watch --noEmit",
    "clean-js":
      "rimraf \"./app/**/*.{js,jsx}\" \"./app/**/*.{js,jsx}.map\" \"./pages/**/*.{js,jsx}\" \"./pages/**/*.{js,jsx}.map\"",
    "rebuild-js": "yarn clean-js && yarn build-ts",
    "clean-jest-cache": "rimraf .jest-cache",
    "clean-environment": "git clean -dfx && yarn install && yarn build-ts",
    "sb": "start-storybook -s . -p 9005 -c .storybook",
    "server-dev": "yarn run build-server && node .server/server.js",
    "dev": "yarn build-server && node .server/server.js",
    "dev:deprecated":
      "yarn run build-server && concurrently -p \"[{name}]\" -n \"TS,NEXT\" -c \"bgBlue,bgMagenta\" \"tsc --watch\" \"node .server/server.js\"",
    "test": "cross-env TZ=America/New_York jest app/* __page_tests__/*",
    "test-pattern": "cross-env TZ=America/New_York jest",
    "test:watch": "cross-env TZ=America/New_York jest --watch",
    "test:watch:coverage": "yarn test --coverage",
    "test:coverage": "yarn test --coverage",
    "test:staged:coverage":
      "cross-env TZ=America/New_York ts-node scripts/coverage.changes.ts",
    "test-ui": "cross-env DEBUG=catalog TZ=America/New_York jest ui-tests/*",
    "test-ui:visible": "cross-env HEADLESS=false yarn test-ui",
    "test-ui:visible:slow":
      "cross-env HEADLESS=false UI_TEST_SPEED=100 yarn test-ui",
    "test-ui:dev":
      "cross-env UI_TEST_PAGE_URL=http://cdahsap03.npd.amerisourcebergen.com:8001 yarn test-ui",
    "test-ui:dev:visible": "cross-env HEADLESS=false yarn test-ui:dev",
    "test-ui:dev:visible:slow":
      "cross-env HEADLESS=false UI_TEST_SPEED=100 yarn test-ui:dev",
    "build": "next build",
    "start": "node .server/server.js",
    "lint": "tslint -p tslint.json --rules-dir tslint-rules",
    "prettify":
      "prettier --write \"**/*.{ts,tsx,json}\" \"server/*.js\" && yarn lint --fix",
    "veracode-zip": "HSS_ZIP=true yarn build && zip -r veracode .next",
    "doc": "typedoc --mode modules --out doc --ignoreCompilerErrors app",
    "setup-dev-server":
      "yarn install --production=false && yarn lint && yarn rebuild-js && yarn test:coverage --no-cache && yarn build-static-stories && yarn build-server && yarn build && npm prune --production",
    "precommit": "",
    "prepush": "yarn build-ts && yarn lint",
    "prod-test":
      "yarn build && yarn build-server && cross-env ASSET_PREFIX=/nhss cross-env NODE_ENV=production yarn start",
    "compile-tslint-rules": "cd tslint-rules && tsc && prettier --write *.js"
  },
  "lint-staged": {
    "gitDir": "./",
    "linters": {
      "**/*.{ts,tsx}": ["prettier --write", "lint --fix", "git add"],
      "**/*.{js,json}": ["prettier --write", "git add"]
    }
  },
